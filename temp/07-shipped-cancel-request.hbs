	<body>
		<table id="body-table" width="100%" border="0" cellpadding="0" cellspacing="0">
			<tr>
				<td align="center" valign="top">
					<table class="w-100 mw6-5 mv4-ns bg-white" width="100%" align="center" border="0" cellpadding="0" cellspacing="0">

            <!-- Header -->
            <tr>
              <td class="w-100 ph4-ns pb4 bb b--light-gray tc">
                <!-- partial:partials/logo.hbs --><img alt="" border="0" width="auto" alt="picpay-empresas-logo.png"
  src="https://lojapicpayb2b.vteximg.com.br/arquivos/picpay-empresas-logo.png" style="max-height: 80px">
<!-- partial -->

                <!-- Just 1 package -->
                {{#compare items.length '==' shippingData.logisticsInfo.length}}
                  {{#compare shippingData.logisticsInfo.0.selectedDeliveryChannel '==' 'pickup-in-point'}}
                    <h1>((i18n.shipping.onePackagePickup))</h1>
                  {{else}}
                    <h1>((i18n.shipping.onePackageDelivery))</h1>
                  {{/compare}}

                  <!-- partial:partials/messages/order-reference.hbs --><div class="mt2 gray">
  ((i18n.order.reference)) <span class="fw7">#{{orderId}}</span>
</div>
<!-- partial -->
                  <!-- partial:partials/messages/cant-cancel.hbs --><div class="bg-washed-red mid-gray ph3 pv3 mt4 fw7 dib w-70-ns center lh-copy">
  ((i18n.common.cantCancel))
</div>
<!-- partial -->

                <!-- More than 1 package -->
                {{else}}
                  {{#compare shippingData.logisticsInfo.0.selectedDeliveryChannel '==' 'pickup-in-point'}}
                    <h1>((i18n.shipping.manyPackagesPickup))</h1>
                  {{else}}
                    <h1>((i18n.shipping.manyPackagesDelivery))</h1>
                  {{/compare}}

                  <!-- partial:partials/messages/order-reference.hbs --><div class="mt2 gray">
  ((i18n.order.reference)) <span class="fw7">#{{orderId}}</span>
</div>
<!-- partial -->
                  <!-- partial:partials/messages/cant-cancel.hbs --><div class="bg-washed-red mid-gray ph3 pv3 mt4 fw7 dib w-70-ns center lh-copy">
  ((i18n.common.cantCancel))
</div>
<!-- partial -->

                  <div class="bg-light-gray ph3 pv3 mt4 fw7 dib w-70-ns center lh-copy">
                    <div>((i18n.shipping.manyPackages))</div>
                    <div>((i18n.shipping.notifications))</div>
                  </div>
                {{/compare}}
              </td>
            </tr>

            <tr>
              <td class="w-100 ph4-ns">
                <!-- Intro message: All items shipped -->

                {{#compare shippingData.logisticsInfo.0.selectedDeliveryChannel '==' 'pickup-in-point'}}
                  <!-- partial:partials/messages/pickup.hbs -->{{#compare items.length '==' shippingData.logisticsInfo.length}}
  <p class="mt4">
    <!-- partial:hi.hbs --><div class="fw7 f5 pb12-px">
  {{#if orders}}
    {{#if orders.0.clientProfileData.firstName}}
      {{#compare orders.0.clientProfileData.firstName '!=' 'isAnonymous'}}
        ((i18n.common.hi)), {{orders.0.clientProfileData.firstName}}!
      {{/compare}}
    {{/if}}
  {{else}}
    {{#if clientProfileData.firstName}}
      {{#compare clientProfileData.firstName '!=' 'isAnonymous'}}
        ((i18n.common.hi)), {{clientProfileData.firstName}}!
      {{/compare}}
    {{/if}}
  {{/if}}
</div>
<!-- partial -->
    {{#compare items.length '==' 1}}
      ((i18n.shipping.pickupProduct))
    {{/compare}}
    {{#compare items.length '>' 1}}
      ((i18n.shipping.pickupProducts))
    {{/compare}}
  </p>
{{/compare}}

<!-- Intro message: Just some items shipped -->
{{#compare items.length '!=' shippingData.logisticsInfo.length}}
  <p class="mt4">
    <!-- partial:hi.hbs --><div class="fw7 f5 pb12-px">
  {{#if orders}}
    {{#if orders.0.clientProfileData.firstName}}
      {{#compare orders.0.clientProfileData.firstName '!=' 'isAnonymous'}}
        ((i18n.common.hi)), {{orders.0.clientProfileData.firstName}}!
      {{/compare}}
